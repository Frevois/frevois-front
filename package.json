    "translations:inspect": "node scripts/translations/inspect",
    "changelog:update": "auto-changelog --template=.changelog/regular.hbs",
    "codemod": "node scripts/codemod.js",
    "bundle-analyzer": "vite-bundle-visualizer --sourcemap"
